{
  "name": "promptlog",
  "version": "1.0.1",
  "description": "Version control for your AI prompts",
  "type": "module",
  "bin": {
    "promptlog": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "prompt",
    "cli",
    "version-control",
    "ai",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "clipboardy": "^4.0.0",
    "commander": "^12.0.0",
    "diff": "^5.1.0"
  },
  "devDependencies": {
    "@types/diff": "^5.0.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
